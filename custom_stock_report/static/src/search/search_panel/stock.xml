<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="StockReportSearchPanelCustom" t-inherit="stock.StockReportSearchPanel.Regular" t-inherit-mode="extension" owl="1">
        <xpath expr="//section[2]" position="after">
            <div class="form-group">
                <label for="fromDate">From Date:</label>
                    <input type="date" class="form-control" id="fromDate" name="fromDate"/>
            </div>
                <div class="form-group">
                <label for="toDate">To Date:</label>
                    <input type="date" class="form-control" id="toDate" name="toDate"/>
            </div>
            <button id="submit_stock_report" t-on-click="() => this.submit_stock_report()" type="submit" class="btn btn-primary">Submit</button>
        </xpath>
    </t>


</templates>
